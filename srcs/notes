int i = 0;
	int j = 0;

	if(!data)
		ft_putstr("here\n");
	while (data[i])
	{
		j = 0;
		while (data[i][j].x != INT_STOP)
		{
			ft_putnbr(data[i][j].x);
			ft_putnbr(data[i][j].y);
			ft_putnbr(data[i][j].z);
			ft_putchar(32);
			j++;
		}
		ft_putchar('\n');
		i++;
	}


	matrix with data has stop value;